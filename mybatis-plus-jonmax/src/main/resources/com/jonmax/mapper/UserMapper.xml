<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jonmax.mapper.UserMapper">

    <select id="findMyUser" resultType="com.jonmax.domain.User">
        select * from tb_user where id = #{userId}
    </select>
    <select id="findMyUserWrapper" resultType="com.jonmax.domain.User">
        select * from tb_user ${ew.customSqlSegment}
    </select>
</mapper>